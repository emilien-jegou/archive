{
  "name": "mailer-pwa-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development next dev",
    "build": "next build && next export",
    "start": "next start",
    "storybook-start-dev": "start-storybook -p 6006",
    "storybook": "yarn watch-svg & yarn storybook-start-dev",
    "generate-svg": "npx @svgr/cli --config-file .svgrrc.js --out-dir components/Icons ./assets/icons",
    "watch-svg": "while inotifywait -q -r -e close_write,moved_to,move_self ./assets/icons/ | grep '\\.svg$'; do yarn generate-svg; done",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.5.1",
    "@types/nanoid": "^2.1.0",
    "axios": "^0.21.1",
    "miragejs": "^0.1.41",
    "nanoid": "^3.1.22",
    "next": "10.1.3",
    "next-pwa": "^5.2.13",
    "next-redux-wrapper": "^6.0.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "^7.2.3",
    "styled-components": "^5.2.3",
    "use-media": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@storybook/addon-actions": "^6.2.7",
    "@storybook/addon-essentials": "^6.2.7",
    "@storybook/addon-links": "^6.2.7",
    "@storybook/react": "^6.2.7",
    "@svgr/webpack": "^5.5.0",
    "@tailwindcss/postcss7-compat": "^2.1.0",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "@types/styled-components": "^5.1.9",
    "autoprefixer": "^9",
    "babel-loader": "^8.2.2",
    "babel-plugin-macros": "^3.0.1",
    "babel-plugin-styled-components": "^1.12.0",
    "postcss": "^7",
    "react-is": "^17.0.2",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "twin.macro": "^2.3.2",
    "typescript": "^4.2.4"
  }
}
